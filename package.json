{
    "name": "react-changelog",
    "private": true,
    "scripts": {
        "coverage-ff": "start firefox \"%cd%\\tests\\reports\\index.html\"",
        "coverage-chrome": "start chrome \"%cd%\\tests\\reports\\index.html\"",
        "dev": "vite",
        "build": "vite build",
        "watch": "vite build --watch",
        "test": "vitest",
        "test-dev": "vitest --coverage",
        "src:fix": "concurrently \"npm run src:format\" \"npm run src:lint\" \"npm run src:lint-test\" --max-processes 1",
        "src:format": "prettier --write \"resources/js/**/*.+(js|jsx|ts|tsx)\"",
        "src:lint-test": "eslint --fix \"tests/Node/**\"",
        "src:lint": "eslint --fix \"resources/js/**\""
    },
    "devDependencies": {
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "@typescript-eslint/eslint-plugin": "^5.30.0",
        "@typescript-eslint/parser": "^5.30.0",
        "@vitejs/plugin-react": "^3.1.0",
        "@vitest/coverage-istanbul": "^0.28.5",
        "axios": "^1.1.2",
        "concurrently": "^7.6.0",
        "eslint": "^8.18.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "laravel-vite-plugin": "^0.7.2",
        "minimist": "^1.2.7",
        "node-forge": "^1.3.1",
        "postcss": "^8.4.21",
        "prettier": "^2.7.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sass": "^1.58.0",
        "sass-loader": "^13.2.0",
        "ts-loader": "^9.4.2",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5",
        "vite": "^4.0.0",
        "vitest": "^0.28.5"
    }
}
